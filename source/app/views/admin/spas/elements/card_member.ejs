<div class="CardItem card" id="<%=item._id%>">
  <div class="card-header">
    Member
    <div class="card-actions">
        <a class="" data-action="collapse"><i class="ti-minus"></i></a>
        <a class="btn-close" data-action=""><i class="ti-close"></i></a>
    </div>
  </div>
  <div class="card-body collapse hide">
    <div class="row">
        <div class="col-md-12">
          <div class="form-group">
              <label>Name</label>
              <input type="text" class="form-control" name="name" value="<%=item.name%>"> 
          </div>
        </div>
        <div class="col-md-12 ">
          <div class="form-group form-group-content ">
              <label class="control-label">Content</label>
              <textarea class="summernote" style="z-index: 1;" name="content"><%=item.content%></textarea>
              <small class="form-control-feedback"></small>
          </div>
        </div>
        <div class="col-md-12">
          <div class="form-group">
              <label>Avatar</label>
              <input type="file" class="ImageUpload form-control" name="avatar" id="<%=item._id%>"> 
              <img class="avatar-100" src="<%=urlMediaUpload.imageObj(item.avatar)%>" />
          </div>
        </div>
    </div>
  </div>
</div>