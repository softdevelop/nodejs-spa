<% extend('../../layouts/base', { 
    title: "Spa management",
    subTitle: "Edit",
    css: `
      <link rel="stylesheet" href="/libs/node_modules/dropify/dist/css/dropify.min.css">
      <link href="/libs/dist/css/style.min.css" rel="stylesheet">
      <link rel="stylesheet" type="text/css" href="/libs/node_modules/summernote/dist/summernote-bs4.css">
      <link rel="stylesheet" href="https://unpkg.com/leaflet@1.6.0/dist/leaflet.css" integrity="sha512-xwE/Az9zrjBIphAcBb3F6JVqxf46+CDLwfLMHloNu6KEQCAWi6HcDUbeOfBIptF7tcCzusKFjFw2yuvEpDL9wQ==" crossorigin=""/>
      <script src="https://unpkg.com/leaflet@1.6.0/dist/leaflet.js" integrity="sha512-gZwIG9x3wUXg2hdXF6+rVkLF/0Vi9U8D2Ntg4Ga5I5BZpVkVxlJWbSQtXPSiUTtC0TjtGOmxa1AJPuV0CPthew==" crossorigin=""></script>
    `,
    script: `
      <script src="/libs/dist/js/custom.min.js"></script>
  
      <script src="/libs/node_modules/summernote/dist/summernote-bs4.min.js"></script>
      <script src="/js/slugify.js"></script>
      <script>
        $(document).ready(function () {
            $(function () {

                $('.summernote').summernote({
                    height: 350, // set editor height
                    minHeight: null, // set minimum height of editor
                    maxHeight: null, // set maximum height of editor
                    focus: false, // set focus to editable area after initializing summernote
                });

                $('.inline-editor').summernote({
                    airMode: true
                });

            });

          
        });
    </script>

    `
    }) %>
<div class="container-fluid">
    <!-- ============================================================== -->
    <!-- Bread crumb and right sidebar toggle -->
    <!-- ============================================================== -->
    <div class="row page-titles">
        <div class="col-md-5 align-self-center">
            <h4 class="text-themecolor">Service</h4>
        </div>
        <div class="col-md-7 align-self-center text-right">
            <div class="d-flex justify-content-end align-items-center">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
                    <li class="breadcrumb-item active">Service</li>
                </ol>
                <a href="/admin/spas/<%=data._id%>/service/index" <button type="button" class="btn btn-info d-none d-lg-block m-l-15"><i
                        class="fa fa-plus-circle"></i> List</button></a>
            </div>
        </div>
    </div>
    <!-- ============================================================== -->
    <!-- End Bread crumb and right sidebar toggle -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- Start Page Content -->
    <!-- ============================================================== -->
    <!-- Row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                    <form action="/admin/spas/<%=data._id%>/service/<%=resole._id%>/edit" method="post" enctype="multipart/form-data">
                        <div class="form-body">
                            <h3 class="card-title">Service Info</h3>
                            <hr>
                            <div class="row p-t-20">
                                <div class="col-md-6">
                                    <div class="form-group  <%=errors.title?'has-danger':''%>">
                                        <label class="control-label">Title</label>
                                        <input type="text" id="title" class="form-control" placeholder="" name="title"
                                             value="<%=resole.title?resole.title:''%>">
                                        <small class="form-control-feedback"><%=errors?errors.title:''%></small>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12 ">
                                    <div class="form-group form-group-content <%=errors.content? 'has-danger': ''%>">
                                        <label class="control-label">content</label>
                                        <textarea class="summernote" style="z-index: 1;" name="content"><%=resole.content?resole.content:''%></textarea>
                                        <small class="form-control-feedback"><%=errors?errors.content:''%></small>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6 ">
                                    <div class="form-group <%=errors.image? 'has-danger': ''%>">
                                       <label class="control-label">Image</label>
                                       <input type="file" id="image" class="form-control" name="image" value="<%=resole.image?resole.image:''%>">
                                       <small class="form-control-feedback"><%=errors?errors.image:''%></small>
                                       <img class="avatar-100" src="<%=urlMediaUpload.imageObj(resole.image)%>" />
          
                                    </div>
                                 </div>
                                <div class="col-md-6 ">
                                    <div class="form-group form-group-price <%=errors.price?'has-danger':''%>">
                                        <label class="control-label">price</label>
                                        <input type="text" id="price" class="form-control" placeholder="" name="price"
                                            required value="<%=resole.price?resole.price:''%>">
                                        <small class="form-control-feedback"><%=errors?errors.slug:''%></small>
                                    </div>
                                </div>

                            </div>
                          
                            <!--/row-->
                            <!--/row-->
                           
                       
                            <!--/row-->
                            <!--/row-->
                       
                            <div class="form-actions">
                                <button type="submit" class="btn btn-success"> <i class="fa fa-check"></i> Save</button>
                                <a href="/admin/spas">
                                    <button type="button" class="btn btn-inverse">Cancel</button>
                                </a>
                            </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <!-- Row -->
    <!-- ============================================================== -->
    <!-- End PAge Content -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- Right sidebar -->
    <!-- ============================================================== -->
