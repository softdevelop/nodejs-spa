<% extend('../layouts/base',{
  title: "Homepage",
}) %>

<!-- ======================================= 
          ==Start Blog section==  
  =======================================-->
<section class="blog-section blog-page">
    <div class="container">
        <div class="row mt-30">
            <div class="col-lg-9 order-lg-2">

                <div class="row">
                    <!--blog 2-->
                    <div class="alert alert-danger mx-auto <%= data.docs.length==0?'':'d-none'  %> " role="alert">
                        Không có dữ liệu!
                    </div>
                    <% data.docs.forEach(function(item) { %>
                    <div class="col-md-4">
                        <div class="single-blog">
                            <div class="blog-img pos-relative">
                                <div class="img-area">
                                    <a href="/tin-tuc/<%=item.slug%>"><img
                                            src="<%=urlMediaUpload.imageObj(item.image)%>" style="height: 200px;"
                                            alt="blog img" class="img-fluid"></a>
                                </div>
                                <ul class="brand">
                                    <li><i class="fa fa-heart"></i>18</li>
                                    <li><i class="fa fa-eye"></i><%=item.numOfViews%></li>
                                    <!-- <li><i class="fa fa-folder-open"></i>Spa Tips</li>ontent -->
                                    <li class="gallery"><i class="fa fa-picture-o"></i></li>
                                </ul>
                                <div class="date">
                                    <h5 class="fw-700 color-ff"><%=moment(item.date).format('DD')%></h5>
                                    <span class="fw-500 roboto color-ff ">Tháng
                                        <%=moment(item.date).format('MM')%></span>
                                </div>
                            </div>
                            <div class="blog-content">
                                <a href="/tin-tuc/<%=item.slug%>" class="title fw-500 pt-20"><%=item.name%></a>
                                <!-- <div class="cut-content" ><%-item.content.substring(0,275)%>...</div> -->
                                <div class="cut-content"><%-truncate(item.content, 100)%></div>
                                <a href="/tin-tuc/<%=item.slug%>" class="readmore" title="Xem thêm" target="-blank">Xem
                                    thêm <i class="fa fa-angle-right"></i></a>
                            </div>
                        </div>
                    </div>
                    <% }) %>
                    <!--blog 3-->


                    <!--/col-->
                </div>
                <!--/row-->

                <div class="row <%= data.docs.length==0?'d-none':''  %>">
                    <div class="col-12 ">
                        <%-genHtmlPaginationClient%>
                    </div>
                </div>
                <!--/row-->
            </div>
            <!--/col-->

            <div class="col-lg-3 order-lg-1">
                <aside class="blog-sidebar">
                    <div class="search-widget single-widget" style="margin-bottom: 20px;">
                        <form action="#" method="post">
                            <input type="text" name="search" placeholder="Tìm kiếm">
                            <button type="submit"><i class="fa fa-search"></i></button>
                        </form>
                    </div>

                    <div class="post-categories single-widget" style="margin-bottom: 0;">
                        <h5 class="widget-title fw-500 pos-relative">Danh mục</h5>
                        <div class="product-gallery with-sidebar">
                            <aside class="sidebar order-lg-1">
                                <div class="single-block categorie">
                                    <ul>
                                        <li><a class="link" href="/tin-tuc">Tất cả</a></li>
                                        <%-genCategory%>
                                    </ul>
                                </div>
                            </aside>
                        </div>
                        <div class="panel-group" id="accordion">
                            <div class="accordion" id="product-categorie">
                            </div>
                        </div>
                    </div>
                    <!--/post categories-->

                    <div class="latest-blog-area single-widget">
                        <h5 class="widget-title fw-500 pos-relative">Tin tức mới nhất</h5>
                        <% newsLatest.forEach(item=>{ %>
                        <div class="single-latest-post">
                            <a href="/tin-tuc/<%=item.slug%>"><img src="<%=urlMediaUpload.imageObj(item.image)%>"
                                    style="max-width: 70px;" alt="latest-blog"></a>
                            <ul>
                                <li>
                                    <h6><a href="/tin-tuc/<%=item.slug%>"><%=item.name%></a></h6>
                                </li>
                                <!-- <li><a href="#">Làm đẹp</a></li> -->
                                <li><%=moment(item.date).format('DD/MM/YYYY')%></li>
                            </ul>
                        </div>
                        <% }) %>
                    </div>
                    <!-- /latest blog area-->

                    <div class="post-categories single-widget">
                        <h5 class="widget-title fw-500 pos-relative">Lưu trữ</h5>
                        <ul>
                            <li><a href="#">Tháng 1 - 2020</a></li>
                            <li><a href="#">Tháng 2 - 2020</a></li>
                            <li><a href="#">Tháng 3 - 2020</a></li>
                            <li><a href="#">Tháng 4 - 2020</a></li>
                            <li><a href="#">Tháng 5 - 2020</a></li>
                            <li><a href="#">Tháng 6 - 2020</a></li>
                            <li><a href="#">Tháng 7 - 2020</a></li>
                            <li><a href="#">Tháng 8 - 2020</a></li>
                            <li><a href="#">Tháng 9 - 2020</a></li>
                            <li><a href="#">Tháng 10 - 2020</a></li>
                            <li><a href="#">Tháng 11 - 2020</a></li>
                            <li><a href="#">Tháng 12 - 2020</a></li>
                        </ul>
                    </div>
                    <!--/archives-->

                    <div class="instagram-gallery single-widget">
                        <h5 class="widget-title fw-500 pos-relative">Instagram</h5>
                        <a href="#"><img src="/images/insta-img/insta-img1.jpg" alt="insta-img" /></a>
                        <a href="#"><img src="/images/insta-img/insta-img2.jpg" alt="insta-img" /></a>
                        <a href="#"><img src="/images/insta-img/insta-img3.jpg" alt="insta-img" /></a>
                        <a href="#"><img src="/images/insta-img/insta-img4.jpg" alt="insta-img" /></a>
                        <a href="#"><img src="/images/insta-img/insta-img5.jpg" alt="insta-img" /></a>
                        <a href="#"><img src="/images/insta-img/insta-img6.jpg" alt="insta-img" /></a>
                        <a href="#"><img src="/images/insta-img/insta-img7.jpg" alt="insta-img" /></a>
                        <a href="#"><img src="/images/insta-img/insta-img8.jpg" alt="insta-img" /></a>
                        <a href="#"><img src="/images/insta-img/insta-img9.jpg" alt="insta-img" /></a>
                    </div>
                    <!--/instagram gallery-->
                </aside>
            </div>
            <!--/col-->
        </div>
        <!--/row-->
    </div>
</section>
<!-- ======================================= 
        ==End blog section== 
=======================================-->

