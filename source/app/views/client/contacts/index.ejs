    <!--bootstrap js-->
    <script src="/libs_client/js/popper.min.js"></script>
    <script src="/libs_client/js/bootstrap.min.js"></script>

    <!--jquery-ui js-->
    <script src="/libs_client/js/jquery-ui.min.js"></script>

    <!--menuzord js-->
    <script src="/libs_client/js/menuzord.js"></script>

    <!--slick js-->
    <script src="/libs_client/js/slick.js"></script>

    <!--owl.carousel js-->
    <script src="/libs_client/js/owl.carousel.min.js"></script>

    <!--nice-select js-->
    <script src="/libs_client/js/jquery.nice-select.min.js"></script>

    <!--venobox js-->
    <script src="/libs_client/js/venobox.min.js"></script>

    <!--counterup js-->
    <script src="/libs_client/js/jquery.counterup.min.js"></script>
    <script src="/libs_client/js/waypoints.min.js"></script>

