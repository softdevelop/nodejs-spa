                                <% }) %>
                              </select>
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6">
                                        <div class="form-group"> <input required class="form-control no-border" id="date" type="date" name="date" placeholder="Ngày đặt*" /></div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 col-lg-12">
                                <div class="form-group"><textarea required class="form-control no-border" id="message" rows="7" name="message" placeholder="Nội dung*"></textarea></div>
                            </div>
                            <div class="col-md-12 col-lg-12">
                                <div class="form-group m-0"> <input required class="btn btn-black" id="send" type="submit" value="Gửi" /></div>
